        void check_click(object sender, EventArgs e)
        {
            if (t1.Text == "BANANA")
            {
                score += 1;
                l2.Text = score.ToString();
                MessageBox.Show("Congratulations!!! You hav guessed it right....", "Spelling Jumble", MessageBoxButtons.OK, MessageBoxIcon.Information);
            }
            else
            {   wrong += 1;
                l4.Text = wrong.ToString();
                MessageBox.Show("Oops..Wrong.. ", "Spelling Jumble", MessageBoxButtons.OK, MessageBoxIcon.Warning);
            }
            t1.Text = "";
            bu1.Visible = false;
            bu2.Visible = false;
            bu3.Visible = false;
            bu4.Visible = false;
            bu5.Visible = false;
            bu6.Visible = false;
