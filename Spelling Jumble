            }
            t1.Text = "";
            bu30.Visible = false;
            bu31.Visible = false;
            bu32.Visible = false;
            bu33.Visible = false;
            bu34.Visible = false;
            bu34.Enabled = false;
            bu35.Visible = false;
            bu36.Visible = false;
            bu37.Visible = false;
            bc5.Enabled = false;
            bc5.Visible = false;
            if (p1.Image != null)
            {
                p1.Image.Dispose();
                p1.Image = null;
            }
            string message = "You Scored " + score + " out of 5";
            if (MessageBox.Show(message, "Score", MessageBoxButtons.OKCancel) == DialogResult.OK)
            {
                if (MessageBox.Show("Do you want to Retry? ", "Retry", MessageBoxButtons.YesNo) == DialogResult.No)
                {
                    
                    this.Close();
                }
                else
                {
                    Form3 NewForm = new Form3();
                    NewForm.Show();
                    this.Dispose(false);
                }
            }
        }
