/*
FORM 1 – Name with start button
using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace shapecolormatch
{
    public partial class Form1 : Form
    {
        public Form1()
        {
            InitializeComponent();
        }
        Button b1 = new Button();
        Label l1 = new Label();
        private void Form1_Load(object sender, EventArgs e)
        {
            this.Location = new Point(0, 0);
            this.Size = Screen.PrimaryScreen.WorkingArea.Size;
            this.BackColor = Color.SkyBlue;
            BackgroundImage = new Bitmap("D:/Wonders/screen-8.jpg");
            BackgroundImageLayout = ImageLayout.Stretch;
            Image picture;
            ImageLayout layout;
            picture = BackgroundImage;
            layout = BackgroundImageLayout;
            this.BackgroundImage = picture;
            this.BackgroundImageLayout = layout;
            
            l1.AutoSize = true;
            l1.Text = "Shape Color Match";
            l1.BackColor = Color.LawnGreen;
            l1.BorderStyle = BorderStyle.Fixed3D;
            l1.Font = new Font("Showcard Gothic", 50, FontStyle.Bold);
            this.Controls.Add(l1);
            l1.Location = new Point(310,550);
            b1.Text = "Click Here";
            b1.Location = new Point(430,50);
            b1.Font = new Font("Goudy Stout", 28, FontStyle.Bold);
            b1.AutoSize = true;
            b1.BackColor = Color.LightCoral;
            this.Controls.Add(b1);
            b1.Click += new EventHandler(button1_click);
        }
        void button1_click(object sender, EventArgs e)
        {
            Form f1 = new Form2();
            f1.Show();
        }
        private void timer1_Tick(object sender, EventArgs e)
        {
            Random r1 = new Random();
            int red = r1.Next(0, 255);
            int green = r1.Next(0, 255);
            int blue = r1.Next(0, 255);
            l1.ForeColor = Color.FromArgb(red, green, blue);
            b1.ForeColor = Color.FromArgb(red, green, blue);
        }
    }
}

